{
  "name": "@kyleupton/node-rsync",
  "version": "1.0.0",
  "description": "Rsync API for server-side javascript runtimes",
  "repository": {
    "type": "git",
    "url": "https://github.com/kyleaupton/node-rsync.git"
  },
  "type": "module",
  "main": "dist/cjs/index.js",
  "types": "dist/cjs/index.d.ts",
  "exports": {
    "require": "./dist/cjs/index.js",
    "import": "./dist/esm/index.js"
  },
  "scripts": {
    "build": "rm -rf dist && tsc --build tsconfig.esm.json && tsc --build tsconfig.cjs.json"
  },
  "author": "Kyle Upton <kyleaupton@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^20.6.2",
    "@typescript-eslint/eslint-plugin": "^6.4.0",
    "eslint": "^8.0.1",
    "eslint-config-standard-with-typescript": "^39.0.0",
    "eslint-plugin-import": "^2.25.2",
    "eslint-plugin-n": "^15.0.0 || ^16.0.0 ",
    "eslint-plugin-promise": "^6.0.0",
    "typescript": "*"
  }
}